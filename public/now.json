{
    "version": 2,
    "alias": "productionweaver",
    "name": "productionweaver-app",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ],
    "build": {
        "env": {
            "API_BASE_URL": "@react_app_api_base_url"
        }
    }
}